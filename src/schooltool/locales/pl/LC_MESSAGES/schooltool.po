#. "\"Monday\",\"B\"\n"
#. "\"Tuesday\",\"C\"\n"
#. "\"***\"\n"
#. "\"johnny\"\n"
#. "\"billy\"\n"
#. "\"\"\n"
#. "\"literature\",\"guzman\"\n"
#. "\"Wednesday\",\"B\"\n"
#. "\"Wednesday\",\"C\"\n"
#. "\"***\"\n"
#. "\"johnny\"\n"
