"\n"
"\"2005-fall\",\"three-day\"\n"
"\"\",\"\",\"\"\n"
"\"philosophy\",\"lorch\"\n"
"\"Monday\",\"A\"\n"
"\"Monday\",\"B\"\n"
"\"Tuesday\",\"C\"\n"
"\"***\"\n"
"\"johnny\"\n"
"\"billy\"\n"
"\"\"\n"
"\"literature\",\"guzman\"\n"
"\"Wednesday\",\"B\"\n"
"\"Wednesday\",\"C\"\n"
"\"***\"\n"
