<notes xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:tal="http://xml.zope.org/namespaces/tal"
          xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="schooltool">
  <title><span tal:replace="context/title"/> Notes</title>
  <tal:loop repeat="note view/getNotes">
      <item xlink:type="simple"
          xlink:title="" xlink:href=""
          tal:attributes="xlink:title note/title;
          xlink:href note/@@path" />
  </tal:loop>
</notes>
