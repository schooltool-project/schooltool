<html metal:use-macro="view/macros/page" i18n:domain="schooltool">
<head>
  <title metal:fill-slot="title" i18n:translate="">Edit address info</title>
</head>
<body>
  <h1 metal:fill-slot="h1" i18n:translate="">
    Edit address info for <span tal:replace="context/title" i18n:name="title" />
  </h1>
  
  <metal:block metal:fill-slot="content" tal:define="info view/info">

  <form method="POST" tal:attributes="action request/path">
  <!--
    <div class="error" tal:condition="view/error" tal:content="view/error" />
    -->
    <div class="row">
    <strong i18n:translate="">Address:</strong>
    <!--
      <span tal:replace="python:context.__name__">Address Name</span>
    -->
    </div>
    
    <div class="row" tal:replace="structure view/country_widget" />
    <div class="row" tal:replace="structure view/streetNr_widget" />
    <div class="row" tal:replace="structure view/thoroughfareName_widget" />  
    <div class="row" tal:replace="structure view/town_widget" />        
    <div class="row" tal:replace="structure view/district_widget" />
    <div class="row" tal:replace="structure view/postcode_widget" />
    
    
    <div class="buttons">
    
      <input class="button" type="submit" name="SAVE" value="Save"
             i18n:attributes="value" />
    </div>
  </form>
  </metal:block>
</body>
</html>
