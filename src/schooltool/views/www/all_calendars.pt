<html i18n:domain="schooltool">
<head>
  <title i18n:translate="">Calendars</title>
</head>
<body>
  <h1 i18n:translate="">Calendars</h1>
  <h2 i18n:translate="">Groups</h2>
  <ul>
    <li tal:repeat="item view/groups">
      <a tal:attributes="href item/href"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(private calendar)</tal:block
      ></a>,
      <a tal:attributes="href item/tthref"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(timetable)</tal:block
      ></a>
    </li>
  </ul>
  <h2 i18n:translate="">Persons</h2>
  <ul>
    <li tal:repeat="item view/persons">
      <a tal:attributes="href item/href"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(private calendar)</tal:block
      ></a>,
      <a tal:attributes="href item/tthref"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(timetable)</tal:block
      ></a>
    </li>
  </ul>
  <h2 i18n:translate="">Resources</h2>
  <ul>
    <li tal:repeat="item view/resources">
      <a tal:attributes="href item/href"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(private calendar)</tal:block
      ></a>,
      <a tal:attributes="href item/tthref"
        ><span tal:replace="item/title"
        /> <tal:block i18n:translate="">(timetable)</tal:block
      ></a>
    </li>
  </ul>
</body>
</html>
