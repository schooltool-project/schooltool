  <adapter factory=".timetable.RemoveTimetablesOnTermDeletion"
           name="remove_timetables_when_term_is_deleted" />

  <adapter factory=".timetable.EmergencyDayTimetableSubscriber"
           name="update_emergency_day_timetables" />

</configure>
