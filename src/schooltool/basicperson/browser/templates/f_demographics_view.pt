<div i18n:domain="schooltool">
  <table>
    <thead>
      <tr>
        <th i18n:translate="">Title</th>
        <th i18n:translate="">ID</th>
        <th i18n:translate="">Type</th>
        <th i18n:translate="">Required</th>
        <th i18n:translate="">Limited</th>
        <th i18n:translate="">Teach.</th>
        <th i18n:translate="">Stud.</th>
        <th i18n:translate="">Admin.</th>
      </tr>
    </thead>
    <tr tal:repeat="row view/table">
      <td tal:content="row/title" />
      <td tal:content="row/id" />
      <td tal:content="row/type" />
      <td tal:content="row/required" />
      <td tal:content="row/limited" />
      <td tal:content="row/teacher" />
      <td tal:content="row/student" />
      <td tal:content="row/admin" />
    </tr>
  </table>
</div>

